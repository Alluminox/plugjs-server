{
  "name": "plugjs-server",
  "version": "1.0.8",
  "main": "./dist/index.js",
  "description": "Plugjs server, base on create server loading configs!",
  "bin": {
    "plugjs-server": "bin/plugjs-server.js"
  },
  "peerDependencies": {
    "dotenv-safe": "8.x",
    "uuid": "3.x"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "sucrase ./lib -d ./dist --transforms imports",
    "build:cjs": "rollup --config config/cjs.config.js",
    "build:esm": "rollup --config config/esm.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "nodemon": "^2.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "sucrase": "^3.12.1"
  },
  "dependencies": {
    "dotenv-safe": "^8.2.0",
    "uuid": "^3.4.0"
  },
  "license": "ISC",
  "preferGlobal": true,
  "repository": {
    "type": "git",
